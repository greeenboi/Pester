{
  "tasks": {
    "dev": "deno run --allow-net --watch main.ts",
    "start": "deno run --allow-net main.ts",
    "test": "deno test --allow-net main_test.ts",
    "build": "deno compile --allow-net --output pester_server main.ts",
    "all": "deno task test && deno task build"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}
